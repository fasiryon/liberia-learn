    "routing_logic": {
      "description": "How the Factory Orchestrator handles different EWO types",
      "rules": [
        {
          "ewo_type": "SINGLE_LESSON",
          "behavior": "Linear pipeline through all required agents",
          "parallelization": false,
          "child_generation": false
        },
        {
          "ewo_type": "UNIT",
          "behavior": "Generate child SINGLE_LESSON EWOs after architecture stage",
          "parallelization": true,
          "child_generation": true,
          "note": "Research + Architect run once for unit. Then spawn N lesson EWOs."
        },
        {
          "ewo_type": "TERM",
          "behavior": "Generate child UNIT EWOs after architecture stage",
          "parallelization": true,
          "child_generation": true,
          "note": "Research + Architect run once for term. Then spawn N unit EWOs."
        },
        {
          "ewo_type": "FULL_SUBJECT",
          "behavior": "Generate child TERM EWOs after architecture stage",
          "parallelization": true,
          "child_generation": true,
          "note": "Research + Architect run once for full subject. Then spawn N term EWOs. This triggers cascade: FULL_SUBJECT → TERMs → UNITs → LESSONs"
        }
      ]
    }
